{
  "name": "on-prem",
  "version": "1.0.0",
  "main": "compiled/main.js",
  "types": "main.ts",
  "license": "Apache-2.0",
  "private": true,
  "scripts": {
    "import": "cdk8s import",
    "synth": "cdk8s synth",
